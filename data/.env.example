# BiliUtility Configuration
# Copy this file to .env and fill in your values

# DeepL API Key for translation (optional)
# Get your key at: https://www.deepl.com/pro-api
DEEPL_AUTH_KEY=your_deepl_api_key_here

# MixItUp Webhook URLs for guard notifications (optional)
# These are triggered when someone purchases captain/admiral/governor
MIXITUP_WEBHOOK_URL_CAPTAIN=
MIXITUP_WEBHOOK_URL_ADMIRAL=
MIXITUP_WEBHOOK_URL_GOVERNOR=

# Default Room ID (optional, can be configured via web interface)
ROOM_ID=

# Default UID (optional, can be configured via web interface)
UID=
